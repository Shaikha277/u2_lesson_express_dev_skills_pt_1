<!DOCTYPE html>
<html>
    <head>
      <link rel='stylesheet' href='/stylesheets/style.css' />
        <title><%= title %></title>
        
    </head>
    <body>
        <h1> TO GET DONE</h1>
        <ul>
          <a href="/skills/new">Add Skill</a>
          
        <% skills.forEach( skill => { %>
          <li>
            <a href="/skills/<%= skill.id %>/edit">Edit Skill</a>
            <form action="/skills/<%= skill.id %>?_method=DELETE" class="delete-form" method="POST">
                <button type="submit">X</button>
            </form>
            <a href="/skills/<%= skill.id %>"><%= skill.skill %></a>
            <% }); %>
        </li>

        </ul>
    </body>
  <html>